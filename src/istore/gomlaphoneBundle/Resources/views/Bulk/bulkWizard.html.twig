<div class="col-md-10">
        <div class="form-horizontal" id="bulkForm">
                <div class="form-container">
                        <div class="">
                                <div class="row input-serial alert alert-info alert-serial-model alert-serial-model-override">
                                        <div class="col-lg-7 col-lg-offset-2">
                                                <div class="form-group">
                                                        <input type="text" class="form-control input-lg" id="bulkSerial" name="bulkSerial[{{i}}]" placeholder="{% trans %}Enter Model Serial{% endtrans %} {{i}}">
                                                        <label for="bulkSerial" class="bulk-error"></label>
                                                        <input type="hidden" class="form-control" id="modelId" name="modelId" value="">
                                                        <input type="hidden" class="form-control" id="bulkItemHasSerial" name="bulkItemHasSerial" value="">
                                                </div>
                                        </div>
                                        <div class="col-lg-1">
                                                <button id="check-model-serial" data-loading-text="{% trans %}Loading{% endtrans %}..." class="btn btn-warning btn-lg" type="button">{% trans %}Check{% endtrans %}</button>
                                        </div>
                                </div>
                                <div class="row">
                                        <div class="col-md-6">
                                                <div class="form-group">
                                                        <label for="bulkModelBrand" class="col-sm-6 control-label">{% trans %}Model Brand{% endtrans %}</label>
                                                        <div class="col-sm-6">
                                                                <input type="text" class="form-control" disabled id="bulkModelBrand" name="bulkModelBrand" placeholder="{% trans %}Model Brand{% endtrans %}">
                                                        </div>
                                                </div>
                                                <div class="form-group">
                                                        <label for="bulkModelName" class="col-sm-6 control-label">{% trans %}Model Name{% endtrans %}</label>
                                                        <div class="col-sm-6">
                                                                <input type="text" class="form-control" disabled id="bulkModelName" name="bulkModelName" placeholder="{% trans %}Model Name{% endtrans %}">
                                                                <!--<label id="bulkModel" name="bulkModel" class="col-sm-12 control-label"></label>-->
                                                        </div>
                                                </div>
                                                <div class="form-group">
                                                        <label for="bulkModelNumber" class="col-sm-6 control-label">{% trans %}Model Number{% endtrans %}</label>
                                                        <div class="col-sm-6">
                                                                <input type="text" class="form-control" disabled id="bulkModelNumber" name="bulkModelNumber" placeholder="{% trans %}Model Number{% endtrans %}">
                                                                <!--<label id="bulkModel" name="bulkModel" class="col-sm-12 control-label"></label>-->
                                                        </div>
                                                </div>
                                                <div class="form-group">
                                                        <label for="bulkCategory" class="col-sm-6 control-label">{% trans %}Category{% endtrans %}</label>
                                                        <div class="col-sm-6">
                                                                <input type="text" class="form-control" disabled id="bulkCategory" name="bulkCategory" placeholder="{% trans %}Category{% endtrans %}">
                                                                <!--<label id="bulkCategory" name="bulkCategory" class="col-sm-12 control-label"></label>-->
                                                        </div>
                                                </div>
                                        </div>
                                        <div class="col-md-6">
                                                <div class="form-group">
                                                        <label for="bulkBuyPrice" class="col-sm-5 control-label">{% trans %}Buy Price{% endtrans %}</label>
                                                        <div class="col-sm-6">
                                                                <input type="number" class="form-control" id="bulkBuyPrice" name="bulkBuyPrice[{{i}}]" placeholder="{% trans %}Price L.E.{% endtrans %}">
                                                                <label for="bulkBuyPrice" class="bulk-error"></label>
                                                        </div>
                                                </div>
                                                <div class="form-group">
                                                        <label for="bulkSellPrice" class="col-sm-5 control-label">{% trans %}Sell Price{% endtrans %}</label>
                                                        <div class="col-sm-6">
                                                                <input type="number" class="form-control" id="bulkSellPrice" name="bulkSellPrice[{{i}}]" placeholder="{% trans %}Price L.E.{% endtrans %}">
                                                                <label for="bulkSellPrice" class="bulk-error"></label>
                                                        </div>
                                                </div>
                                                <div class="form-group">
                                                        <label for="bulkQuantity" class="col-sm-5 control-label">{% trans %}Quantity{% endtrans %}</label>
                                                        <div class="col-sm-4">
                                                                <input type="number" class="form-control" id="bulkQuantity" name="bulkQuantity[{{i}}]" placeholder="{% trans %}Quantity{% endtrans %}" value="1">
                                                                <label for="bulkQuantity" class="bulk-error"></label>
                                                        </div>
                                                </div>
                                        </div>
                                </div>
                        </div>
                </div>
        </div>
</div>