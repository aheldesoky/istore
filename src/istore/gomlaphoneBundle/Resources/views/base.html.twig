{% set lang = 'en' %}
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html" charset=utf-8" />
        <link rel="shortcut icon" href="/bundles/istoregomlaphone/img/mobile.png" />
        <!--[if lt IE 9]>
            <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
        <title>{% block title %}Gomla Phone Store Management System{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
            {% block javascripts %}
                <script src="/bundles/istoregomlaphone/js/ar.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery-2.1.0.min.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery.flipcountdown.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery-ui.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery.validate.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery.validate.additional-methods.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery.form.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery.fileDownload.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery.bootstrap.wizard.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery.autocomplete.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery.tablesorter.min.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery.tablesorter.widgets.min.js"></script>
                <script src="/bundles/istoregomlaphone/js/validation-messages.js"></script>
                <script src="/bundles/istoregomlaphone/js/moment.min.js"></script>
                <script src="/bundles/istoregomlaphone/js/bootstrap-datetimepicker.min.js"></script>
                <script src="/bundles/istoregomlaphone/js/bootstrap-datetimepicker.ar.js"></script>
                <!--<script src="/bundles/istoregomlaphone/js/bootstrap.min.js"></script>-->
                <script src="/bundles/istoregomlaphone/js/bootstrap-rtl.min.js"></script>
                <script src="/bundles/istoregomlaphone/js/bootstrap-paginator.min.js"></script>
                <script src="/bundles/istoregomlaphone/js/tooltipsy.min.js"></script>
                <script src="/bundles/istoregomlaphone/js/json2.min.js"></script>
                <script src="/bundles/istoregomlaphone/js/jquery.slimscroll.min.js"></script>
                <script src="/bundles/istoregomlaphone/js/prettify.js"></script>
                <script src="/bundles/istoregomlaphone/js/main.js"></script>
            {% endblock %}
            {% block stylesheets %}
                <!--<link href="/bundles/istoregomlaphone/css/bootstrap.min.css" media="screen" rel="stylesheet" type="text/css">-->
                <link href="/bundles/istoregomlaphone/css/bootstrap-rtl.min.css" media="screen" rel="stylesheet" type="text/css">
                <!--<link href="/bundles/istoregomlaphone/css/bootstrap-theme-rtl.min.css" media="screen" rel="stylesheet" type="text/css">-->
                <link rel="stylesheet" type="text/css" href="/bundles/istoregomlaphone/css/jquery.flipcountdown.css" />
                <link rel="stylesheet" href="/bundles/istoregomlaphone/css/bootstrap-datetimepicker.min.css">
                <link rel="stylesheet" href="/bundles/istoregomlaphone/css/jquery-ui.css">
                <link rel="stylesheet" href="/bundles/istoregomlaphone/css/prettify.css">
                <link rel="stylesheet" href="/bundles/istoregomlaphone/css/style.css">
            {% endblock %}

    </head>
    <body>
        <div class="wrapper">
            <header id="header">
                <!--<a class="btn btn-md btn-hide-nav"></a>-->
                {% block navigation %}
                <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
                    <div class="container">
                        <div class="navbar-header">
                          <a href="/" class="navbar-brand">{% trans %}Gomla Phone{% endtrans %}</a>
                          <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-main">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                          </button>
                        </div>
                        <div class="navbar-collapse collapse" id="navbar-main">
                          <ul class="nav navbar-nav {% if lang=='ar' %}ar{% endif %}">
                                <li class="home"><a href="/">{% trans %}Home{% endtrans %}</a></li>
                                <li class="stock"><a href="/stock">{% trans %}Stock{% endtrans %}</a></li>
                                <li class="sale"><a href="/sale/prepaid">{% trans %}Sales{% endtrans %}</a></li>
                                <li class="supplier"><a href="/supplier">{% trans %}Suppliers{% endtrans %}</a></li>
                                <li class="customer"><a href="/customer">{% trans %}Customers{% endtrans %}</a></li>
                                <li class="warranty"><a href="/warranty">{% trans %}Warranties{% endtrans %}</a></li>
                                <li class="report"><a href="/report">{% trans %}Reports{% endtrans %}</a></li>
                                <li class="settings dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">{% trans %}Settings{% endtrans %} <b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li class="category"><a href="/category">{% trans %}Categories{% endtrans %}</a></li>
                                        <li class="color"><a href="/color">{% trans %}Colors{% endtrans %}</a></li>
                                        <li class="color"><a href="/brand">{% trans %}Brands{% endtrans %}</a></li>
                                        <li class="model"><a href="/model">{% trans %}Models{% endtrans %}</a></li>
                                    </ul>
                                </li>
                                {% if 'ROLE_ADMIN' in app.user.roles %}
                                        
                                        <!--<li class="bulk"><a href="/bulk">{% trans %}Bulks{% endtrans %}</a></li>-->
                                        <!--<li class="item"><a href="/item">{% trans %}Items{% endtrans %}</a></li>-->
                                        
                                {% endif %}
                                {% if 'ROLE_SUPER_ADMIN' in app.user.roles %}
                                <!--<li class="user"><a href="/user">{% trans %}Users{% endtrans %}</a></li>-->
                                {% endif %}
                          </ul>
                          <!-- Language button -->
                            <!--<div class="btn-group lang-btn {% if lang=='ar' %}left{% else %}right{% endif %}">
                              <button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
                                Language <span class="caret"></span>
                              </button>
                              <ul class="dropdown-menu" role="menu">
                                <li><a href="?lang=ar">Arabic</a></li>
                                <li><a href="?lang=en">English</a></li>
                              </ul>
                            </div>-->
                            <div class="">
                                {% if app.user %}
                                    <ul class="nav navbar-nav navbar-right user-nav">
                                        <li class="dropdown">
                                          <a href="#" class="dropdown-toggle" data-toggle="dropdown">{{ app.user.name }} <b class="caret"></b></a>
                                          <ul class="dropdown-menu">
                                            <li><a href="/profile">{%trans%}Profile{%endtrans%}</a></li>
                                            <li><a href="/logout">{%trans%}Logout{%endtrans%}</a></li>
                                          </ul>
                                        </li>
                                    </ul>
                                {% endif %}
                                
                            </div>
                        </div><!--/.nav-collapse -->
                    </div>
                </nav>
                {% endblock %}
            </header>
            <section class="main-col">
                <div class="container content page-wrap">
                    <div class="row"><div id="alert-message" class="col-lg-12"></div></div>
                    {% block body %}{% endblock %}
                </div> <!-- /container -->
            </section>
            <div class="push"></div>
        </div>
        
        <div class="footer">
            {% block footer %}
            <footer>
                    &copy; 2014 powered by <a href="http://sourcetect.com" target="_blank">SourceTect</a>. All rights reserved.
            </footer><!-- /footer -->
            {% endblock %}
        </div>
    </body>
</html>
